{
  "name": "jade/jade",
  "description": "A flexible and elegant framework for PHP project",
  "authors": [
    {
      "name": "Slince",
      "email": "taosikai@yeah.net"
    }
  ],
  "config": {
    "platform": {
      "php": "7.1.3"
    }
  },
  "require": {
    "php": "^7.1.3",
    "symfony/event-dispatcher": "^4.3",
    "pimple/pimple": "^3.2",
    "zendframework/zend-stratigility": "^3.2",
    "zendframework/zend-diactoros": "^2.1",
    "zendframework/zend-config-aggregator": "^1.1",
    "nikic/fast-route": "^1.3",
    "symfony/var-dumper": "^4.3",
    "zendframework/zend-httphandlerrunner": "^1.1",
    "symfony/debug": "^4.3",
    "symfony/console": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "suggest": {
    "symfony/validator": "Enable validation",
    "symfony/serializer": "Enable entity serializer"
  },
  "autoload": {
    "psr-4": {
      "Cube\\": "./src/Cube"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cube\\Tests\\": "./tests"
    }
  }
}
