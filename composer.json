{
    "name": "jadephp/jade",
    "description": "Jade is a flexible PHP micro framework",
    "authors": [
        {
            "name": "Slince",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "pimple/pimple": "^3.2",
        "nikic/fast-route": "^1.3",
        "symfony/event-dispatcher": "^4.3",
        "symfony/console": "^4.3",
        "zendframework/zend-stratigility": "^3.2",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/var-dumper": "^4.3",
        "symfony/debug": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Jade\\": "./src/Jade"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jade\\Tests\\": "./tests"
        }
    }
}
